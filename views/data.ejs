<table border="1px">
  <thead>
    <tr>
      <th>Patient ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Date of birth</th>
    </tr>
  </thead>
  <tbody>
    <% results1.forEach(row=>{
      %>
        <tr>
          <td><%=row.patient_id%></td>
          <td><%=row.first_name%></td>
          <td><%=row.last_name%></td>
          <td><%=row.date_of_birth%></td>
        </tr>
      <%
    })%>
  </tbody>
</table>

